    "@babel/plugin-transform-runtime": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@babel/runtime": "^7.22.11",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.1",
    "copy-webpack-plugin": "^11.0.0",
    "cpy-cli": "^5.0.0",
    "css-loader": "^6.8.1",
    "express": "^4.18.2",
    "html-webpack-plugin": "^5.5.3",
    "http-server": "^14.1.1",
    "idb": "^7.1.1",
    "style-loader": "^3.3.3",
    "webpack": "^5.88.2",
